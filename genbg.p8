pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function mrnd(n)
 return rnd(n*2)-n
end

function bg1()
 for i=0,1643 do
  local sx=8
  local x,y=rnd(128),rnd(128)
  local c=sget(sx+x/16,y/16)
  x+=mrnd(3)
  y+=mrnd(3)
  circfill(x,y,1,c)
 end
end

n=0
m=0

function _update()
 if (btnp(4)) n+=1
 if (btnp(5)) m=(m+1)%4
end

function _draw()
 srand(n)
 cls()
 local mw=32
 local mh=32
 local sx=(m+1)*8
 for i=0,400 do
  local x,y=rnd(mw),rnd(mh)
  local w=min(mw-x,rnd(mw/6))
  local h=min(mh-y,rnd(mh/6))
  h=w
  local c=sget(
     sx+x/(mw/8),
     y/(mh/8))
  rectfill(x,y,x+w,y+h,c)
 end
 
 for i=0,30 do
  local x,y=rnd(mw),rnd(mh)
  local w=max(2,rnd(8))
  local h=w
  local c=sget(
     sx+x/(mw/8),
     y/(mh/8))
  x+=rnd(3)
  y+=rnd(3)
  rect(x,y,x+w,y+h,c)
 end
end
